package test;

public class Sample {

	public static void main(String[] args) {

		System.out.println("오마나");


		System.out.println();
		System.out.println();

		int a=10;
		int b=20;
		int c=a;

		char var1='A';
		//char c1=  0xAC63; 
		//char c1=  0xAC63; 
	//	char c1=  0xAC00; 
		char c1=  0xD79F; 

		char var2='가';
		char c2 = 44032;		

		System.out.println(var1);
		System.out.println(c1);
		System.out.println(var2);
		System.out.println(c2);

//BigDecimal - 정교한 소수점을 계산하고 싶을 때 사용한다.
		
	 float f1 = 10.5f;
			 
	double d1 = 10.5;
	
	long l1 = (long) 10.5;
	

	}

}

