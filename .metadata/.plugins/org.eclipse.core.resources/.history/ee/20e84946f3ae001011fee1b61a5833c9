package test;

public class Sample {

	public static void main(String[] args) {

		System.out.println("오마나");


		System.out.println();
		System.out.println();

		int a=10;
		int b=20;
		int c=a;

		char var1='A';
		//char c1=  0xAC63; 
		//char c1=  0xAC63; 
	//	char c1=  0xAC00; 
		char c1=  0xD79F; 

		char var2='가';
		char c2 = 44032;		

		System.out.println(var1);
		System.out.println(c1);
		System.out.println(var2);
		System.out.println(c2);

//BigDecimal - 정교한 소수점을 계산하고 싶을 때 사용한다.
		
	 float f1 = 10.5f;
			 
	double d1 = 10.5;
	
	long l1 = (long) 10.5;
	
	System.out.println(f1);
	System.out.println(d1);
	System.out.println(l1);
	
float f2 = 2147483648f;
float f3 = 2147483658f;
float f4 = 2147483668f;

System.out.println(f2);
System.out.println(f3);
System.out.println(f4);

//2,147,483,648 = 2.1474836E9 = 2.1474836x 10 = 2,147,483,600
// float는 오차 범위가 커서 사용하지 않는 것이 좋다.

double ff2 = 2147483648L;
double ff3 = 2147483648L;
double ff4 = 2147483648L;

System.out.println(ff2);
System.out.println(ff3);
System.out.println(ff4);
	
String str1 = "안\t창\t현";
String str2 = "안\n창\n현";
String str3 = "안\"창\"현";
	
System.out.println(str1);
System.out.println(str2);
System.out.println(str3);
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	}

}

